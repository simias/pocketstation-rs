{
    "arch": "arm",
    "cpu": "arm7tdmi",
    "data-layout": "e-m:e-p:32:32-i64:64-v128:64:128-a:0:32-n32-S64",
    "disable-redzone": true,
    "executables": true,
    "llvm-target": "armv4t-none-eabi",
    "morestack": false,
    "os": "none",
    "relocation-model": "static",
    "target-endian": "little",
    "target-pointer-width": "32",
    "no-compiler-rt": true,
    "pre-link-args": [
	"-mcpu=arm7tdmi",
	"-nostdlib",
	"-Tpocketstation.ld",
	"-Wl,--gc-sections"
    ],
    "post-link-args": [
	"-lgcc"
    ]
}
